<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ui.uiapplication.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.ui.uiapplication.ui.dashboard.MatchFragment"
        android:label="@string/title_match"
        tools:layout="@layout/fragment_match" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.ui.uiapplication.ui.notifications.AttentionFragment"
        android:label="@string/title_attention"
        tools:layout="@layout/fragment_attention" />
    <fragment
        android:id="@+id/navigation_my"
        android:name="com.ui.uiapplication.ui.my.MyFragment"
        android:label="@string/title_my"
        tools:layout="@layout/fragment_attention" />
</navigation>